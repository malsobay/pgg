layer.get(\"avatar\")}\n          className=\"profile-avatar\"\n        />\n      </div>\n    );\n  }\n\n  renderScore() {\n    const { player } = this.props;\n    return (\n      <div>\n        <h4>Total MU</h4>\n        <span>{player.get(\"cumulativePayoff\")}</span>\n      </div>\n    );\n  }\n  \n  renderRoundNumber() {\n    const { round } = this.props;\n    return (\n      <div>\n        <h4>Round Number</h4>\n        <span>{round.index + 1}</span>\n      </div>\n    );\n  }\n\n  renderRoundStage() {\n    const { stage } = this.props;\n    return (\n      <div>\n        <h4>Round Stage</h4>\n        <span>{stage.name}</span>\n      </div>\n    );\n  }\n\n  renderRoundMU() {\n    const { game } = this.props;\n    return (\n      <div>\n        <h4>Round MU</h4>\n        <span>{game.treatment.endowment}</span>\n      </div>\n    );\n  }\n\n  render() {\n    const { stage } = this.props;\n\n    return (\n      <aside className=\"sidebar\">\n        <div className=\"sidebar-stats\">\n          {this.renderProfile()}\n          {this.renderScore()}\n          {this.renderRoundNumber()}\n          {this.renderRoundMU()}\n          {this.renderRoundStage()}\n          <Timer stage={stage} />\n        </div>\n      </aside>\n    );\n  }\n}\n"]},"sourceType":"module","hash":"4ad966b35dee223fc2ab3dd130983a7e98c81383"}
