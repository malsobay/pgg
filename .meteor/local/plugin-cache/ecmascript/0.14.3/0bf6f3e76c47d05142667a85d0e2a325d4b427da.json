mpleSchema = v;\n  }\n}, 0);\nvar moment;\nmodule.link(\"moment\", {\n  \"default\": function (v) {\n    moment = v;\n  }\n}, 1);\nvar BelongsTo, TimestampSchema, UserDataSchema, HasManyByRef;\nmodule.link(\"../default-schemas\", {\n  BelongsTo: function (v) {\n    BelongsTo = v;\n  },\n  TimestampSchema: function (v) {\n    TimestampSchema = v;\n  },\n  UserDataSchema: function (v) {\n    UserDataSchema = v;\n  },\n  HasManyByRef: function (v) {\n    HasManyByRef = v;\n  }\n}, 2);\nvar Games;\nmodule.link(\"../games/games\", {\n  Games: function (v) {\n    Games = v;\n  }\n}, 3);\nvar PlayerStages;\nmodule.link(\"../player-stages/player-stages\", {\n  PlayerStages: function (v) {\n    PlayerStages = v;\n  }\n}, 4);\nvar Rounds;\nmodule.link(\"../rounds/rounds\", {\n  Rounds: function (v) {\n    Rounds = v;\n  }\n}, 5);\nvar Stages = new Mongo.Collection(\"stages\");\nStages.helpers({\n  round: function () {\n    return Rounds.findOne(this.roundId);\n  }\n});\nStages.stagePaddingDuration = moment.duration(0.25, \"seconds\");\nStages.schema = new SimpleSchema({\n  // Index represents the 0 based position of the current stage in the ordered\n  // list of a all the game's stages. For display, add 1.\n  index: {\n    type: SimpleSchema.Integer,\n    min: 0,\n    max: 999999 // That's a lot of stages...\n\n  },\n  name: {\n    type: String,\n    max: 64\n  },\n  displayName: {\n    type: String,\n    max: 128 // TODO Add auto value to by default copy the name into the displayName?\n\n  },\n  // This will synchronize the clients timer start time and record start time\n  // for the record\n  startTimeAt: {\n    type: Date,\n    optional: true\n  },\n  durationInSeconds: {\n    type: SimpleSchema.Integer,\n    // One day, that's a lot, but could be \"weird\" experiment, yet no going nuts\n    // into hundreds of years for example.\n    max: 24 * 60 * 60,\n    // It would be difficult to manage a timer that is less than 5s given all\n    // the multi-peer synchronization going on.\n    min: 5\n  }\n});\nStages.schema.extend(TimestampSchema);\nStages.schema.extend(UserDataSchema);\nStages.schema.extend(BelongsTo(\"Rounds\"));\nStages.schema.extend(BelongsTo(\"Games\"));\nStages.schema.extend(HasManyByRef(\"PlayerStages\"));\nStages.attachSchema(Stages.schema);","map":{"version":3,"sources":["packages/empirica:core/api/stages/stages.js"],"names":["module","export","Stages","SimpleSchema","link","v","moment","BelongsTo","TimestampSchema","UserDataSchema","HasManyByRef","Games","PlayerStages","Rounds","Mongo","Collection","helpers","round","findOne","roundId","stagePaddingDuration","duration","schema","index","type","Integer","min","max","name","String","displayName","startTimeAt","Date","optional","durationInSeconds","extend","attachSchema"],"mappings":"AAAAA,MAAM,CAACC,MAAP,CAAc;AAACC,EAAAA,MAAM,EAAC,YAAU;AAAC,WAAOA,MAAP;AAAc;AAAjC,CAAd;AAAkD,IAAIC,YAAJ;AAAiBH,MAAM,CAACI,IAAP,CAAY,cAAZ,EAA2B;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACF,IAAAA,YAAY,GAACE,CAAb;AAAe;AAApC,CAA3B,EAAiE,CAAjE;AAAoE,IAAIC,MAAJ;AAAWN,MAAM,CAACI,IAAP,CAAY,QAAZ,EAAqB;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACC,IAAAA,MAAM,GAACD,CAAP;AAAS;AAA9B,CAArB,EAAqD,CAArD;AAAwD,IAAIE,SAAJ,EAAcC,eAAd,EAA8BC,cAA9B,EAA6CC,YAA7C;AAA0DV,MAAM,CAACI,IAAP,CAAY,oBAAZ,EAAiC;AAACG,EAAAA,SAAS,EAAC,UAASF,CAAT,EAAW;AAACE,IAAAA,SAAS,GAACF,CAAV;AAAY,GAAnC;AAAoCG,EAAAA,eAAe,EAAC,UAASH,CAAT,EAAW;AAACG,IAAAA,eAAe,GAACH,CAAhB;AAAkB,GAAlF;AAAmFI,EAAAA,cAAc,EAAC,UAASJ,CAAT,EAAW;AAACI,IAAAA,cAAc,GAACJ,CAAf;AAAiB,GAA/H;AAAgIK,EAAAA,YAAY,EAAC,UAASL,CAAT,EAAW;AAACK,IAAAA,YAAY,GAACL,CAAb;AAAe;AAAxK,CAAjC,EAA2M,CAA3M;AAA8M,IAAIM,KAAJ;AAAUX,MAAM,CAACI,IAAP,CAAY,gBAAZ,EAA6B;AAACO,EAAAA,KAAK,EAAC,UAASN,CAAT,EAAW;AAACM,IAAAA,KAAK,GAACN,CAAN;AAAQ;AAA3B,CAA7B,EAA0D,CAA1D;AAA6D,IAAIO,YAAJ;AAAiBZ,MAAM,CAACI,IAAP,CAAY,gCAAZ,EAA6C;AAACQ,EAAAA,YAAY,EAAC,UAASP,CAAT,EAAW;AAACO,IAAAA,YAAY,GAACP,CAAb;AAAe;AAAzC,CAA7C,EAAwF,CAAxF;AAA2F,IAAIQ,MAAJ;AAAWb,MAAM,CAACI,IAAP,CAAY,kBAAZ,EAA+B;AAACS,EAAAA,MAAM,EAAC,UAASR,CAAT,EAAW;AAACQ,IAAAA,MAAM,GAACR