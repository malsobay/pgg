al, we are hacking the internals of account:base, but\n// there are no clea ways to integrate with tha Meteor acccount system.\nMeteor.startup(function () {\n  resetToken();\n}); // override Meteor._localStorage methods and resetToken accordingly\n\nvar originalSetItem = Meteor._localStorage.setItem;\n\nMeteor._localStorage.setItem = function (key, value) {\n  if (key === \"Meteor.loginToken\") {\n    Meteor.defer(resetToken);\n  }\n\n  originalSetItem.call(Meteor._localStorage, key, value);\n};\n\nvar originalRemoveItem = Meteor._localStorage.removeItem;\n\nMeteor._localStorage.removeItem = function (key) {\n  if (key === \"Meteor.loginToken\") {\n    Meteor.defer(resetToken);\n  }\n\n  originalRemoveItem.call(Meteor._localStorage, key);\n};\n\nfunction resetToken() {\n  var loginToken = Meteor._localStorage.getItem(\"Meteor.loginToken\");\n\n  var loginTokenExpires = new Date(Meteor._localStorage.getItem(\"Meteor.loginTokenExpires\"));\n\n  if (loginToken) {\n    setToken(loginToken, loginTokenExpires);\n  } else {\n    setToken(null, new Date());\n  }\n}\n\nfunction setToken(loginToken, expires) {\n  document.cookie = \"meteor_login_token=\" + loginToken + \"; expires=\" + expires.toUTCString() + \"; path=/\";\n}","map":{"version":3,"sources":["packages/empirica:core/startup/client/accounts.js"],"names":["Meteor","startup","resetToken","originalSetItem","_localStorage","setItem","key","value","defer","call","originalRemoveItem","removeItem","loginToken","getItem","loginTokenExpires","Date","setToken","expires","document","cookie","toUTCString"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AAEAA,MAAM,CAACC,OAAP,CAAe,YAAM;AACnBC,EAAAA,UAAU;AACX,CAFD,E,CAIA;;AACA,IAAMC,eAAe,GAAGH,MAAM,CAACI,aAAP,CAAqBC,OAA7C;;AACAL,MAAM,CAACI,aAAP,CAAqBC,OAArB,GAA+B,UAASC,GAAT,EAAcC,KAAd,EAAqB;AAClD,MAAID,GAAG,KAAK,mBAAZ,EAAiC;AAC/BN,IAAAA,MAAM,CAACQ,KAAP,CAAaN,UAAb;AACD;;AACDC,EAAAA,eAAe,CAACM,IAAhB,CAAqBT,MAAM,CAACI,aAA5B,EAA2CE,GAA3C,EAAgDC,KAAhD;AACD,CALD;;AAOA,IAAMG,kBAAkB,GAAGV,MAAM,CAACI,aAAP,CAAqBO,UAAhD;;AACAX,MAAM,CAACI,aAAP,CAAqBO,UAArB,GAAkC,UAASL,GAAT,EAAc;AAC9C,MAAIA,GAAG,KAAK,mBAAZ,EAAiC;AAC/BN,IAAAA,MAAM,CAACQ,KAAP,CAAaN,UAAb;AACD;;AACDQ,EAAAA,kBAAkB,CAACD,IAAnB,CAAwBT,MAAM,CAACI,aAA/B,EAA8CE,GAA9C;AACD,CALD;;AAOA,SAASJ,UAAT,GAAsB;AACpB,MAAMU,UAAU,GAAGZ,MAAM,CAACI,aAAP,CAAqBS,OAArB,CAA6B,mBAA7B,CAAnB;;AACA,MAAMC,iBAAiB,GAAG,IAAIC,IAAJ,CACxBf,MAAM,CAACI,aAAP,CAAqBS,OAArB,CAA6B,0BAA7B,CADwB,CAA1B;;AAIA,MAAID,UAAJ,EAAgB;AACdI,IAAAA,QAAQ,CAACJ,UAAD,EAAaE,iBAAb,CAAR;AACD,GAFD,MAEO;AACLE,IAAAA,QAAQ,CAAC,IAAD,EAAO,IAAID,IAAJ,EAAP,CAAR;AACD;AACF;;AAED,SAASC,QAAT,CAAkBJ,UAAlB,EAA8BK,OAA9B,EAAuC;AACrCC,EAAAA,QAAQ,CAACC,MAAT,2BAAwCP,UAAxC,kBAA+DK,OAAO,CAACG,WAAR,EAA/D;AACD","sourcesContent":["// This page work on saving a login token into a cookie for server requests\n// such as data export.\n//\n// Note: this is suboptimal, we are hacking the internals of account:base, but\n// there are no clea ways to integrate with tha Meteor acccount system.\n\nMeteor.startup(() => {\n  resetToken();\n});\n\n// override Meteor._localStorage methods and resetToken accordingly\nconst originalSetItem = Meteor._localStorage.setItem;\nMeteor._localStorage.setItem = function(key, value) {\n  if (key === \"Meteor.loginToken\") {\n    Meteor.defer(resetToken);\n  }\n  originalSetItem.call(Meteor._localStorage, key, value);\n};\n\nconst originalRemoveItem = Meteor._localStorage.removeItem;\nMeteor._localStorage.removeItem = function(key) {\n  if (key === \"Meteor.loginToken\") {\n    Meteor.defer(resetToken);\n  }\n  originalRemoveItem.call(Meteor._localStorage, key);\n};\n\nfunction resetToken() {\n  const loginToken = Meteor._localStorage.getItem(\"Meteor.loginToken\");\n  const loginTokenExpires = new Date(\n    Meteor._localStorage.getItem(\"Meteor.loginTokenExpires\")\n  );\n\n  if (loginToken) {\n    setToken(loginToken, loginTokenExpires);\n  } else {\n    setToken(null, new Date());\n  }\n}\n\nfunction setToken(loginToken, expires) {\n  document.cookie = `meteor_login_token=${loginToken}; expires=${expires.toUTCString()}; path=/`;\n}\n"]},"sourceType":"module","hash":"8632d46fda8817df2ddb37c6916d74a500e30494"}
